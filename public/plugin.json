{
  "pluginName": "Web开发者助手",
  "description": "Web开发者助手",
  "author": "blowsnow",
  "main": "index.html",
  "preload": "preload.js",
  "logo": "logo.png",
  "version": "1.3.0",
  "features": [
    {
      "code": "open",
      "explain": "web开发者助手",
      "cmds":[
        "web开发者助手","开发者助手"
      ]
    },
    {
      "code": "PostMan",
      "explain": "简易postman",
      "cmds":[
        "postman"
      ]
    },
    {
      "code": "JsonFormat",
      "explain": "JSON格式化",
      "cmds":[
        "JSON格式化",
        {
          "type": "regex",
          "label": "JSON匹配",
          "match": "/^{[\\s\\S]*}|\\[[\\s\\S]*\\]$/i"
        }
      ]
    },
    {
      "code": "CodeFormat",
      "explain": "代码美化",
      "cmds":[
        "代码美化",
        {
          "type": "regex",
          "label": "html匹配",
          "match": "/html|div/i"
        },
        {
          "type": "regex",
          "label": "js匹配",
          "match": "/var|let|function|document|window/i"
        },
        {
          "type": "regex",
          "label": "css匹配",
          "match": "/\\.[\\s\\S]*{[\\s\\S]*}/i"
        },
        {
          "type": "regex",
          "label": "xml匹配",
          "match": "/<\\?xml/i"
        },
        {
          "type": "regex",
          "label": "sql匹配",
          "match": "/select|update|delete|replcace\\s*into|create\\s*table/i"
        }
      ]
    },
    {
      "code": "WordSplit",
      "explain": "分词",
      "cmds":[
        "分词",
        {
          "type": "over",
          "label": "分词匹配"
        }
      ]
    },
    {
      "code": "IpQuery",
      "explain": "ip查询",
      "cmds":[
        "ip查询",
        {
          "type": "regex",
          "label": "ip查询",
          "match": "/^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}$/"
        }
      ]
    },
    {
      "code": "PortScan",
      "explain": "端口扫描",
      "cmds":[
        "端口扫描",
        {
          "type": "regex",
          "label": "端口扫描",
          "match": "/^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}$/"
        }
      ]
    },
    {
      "code": "LocalScan",
      "explain": "局域网扫描",
      "cmds":[
        "局域网扫描",
        {
          "type": "regex",
          "label": "局域网扫描",
          "match": "/^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}$/"
        }
      ]
    },
    {
      "code": "traceroute",
      "explain": "路由跟踪",
      "cmds":[
        "路由跟踪",
        {
          "type": "regex",
          "label": "路由跟踪",
          "match": "/^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}$/"
        },
        {
          "type": "regex",
          "label": "路由跟踪",
          "match": "/(?=^.{4,253}$)(^((?!-)[a-zA-Z0-9-]{1,63}(?<!-)\\.)+[a-zA-Z]{2,63}\\.?$)/"
        }
      ]
    },
    {
      "code": "nslookup",
      "explain": "nslookup",
      "cmds":[
        "nslookup",
        {
          "type": "regex",
          "label": "nslookup",
          "match": "/^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}$/"
        },
        {
          "type": "regex",
          "label": "nslookup",
          "match": "/(?=^.{4,253}$)(^((?!-)[a-zA-Z0-9-]{1,63}(?<!-)\\.)+[a-zA-Z]{2,63}\\.?$)/"
        }
      ]
    },
    {
      "code": "icp",
      "explain": "备案查询",
      "cmds":[
        "icp","备案查询",
        {
          "type": "regex",
          "label": "备案查询",
          "match": "/(?=^.{4,253}$)(^((?!-)[a-zA-Z0-9-]{1,63}(?<!-)\\.)+[a-zA-Z]{2,63}\\.?$)/"
        }
      ]
    }
  ]
}
